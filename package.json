{
  "name": "ex-ng2-home-automation",
  "version": "1.0.0",
  "description": "Express Angular2 Home Automation",
  "main": "server/app.js",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "dev:run": "concurrently --kill-others \"npm run server:run\" \"npm run client:serve\"",
    "prod:run": "forever ./prod/server/bin/www.js",
    "dev:debug": "concurrently --kill-others \"npm run client:serve\" \"npm run server:watch\"",
    "prod:build": "npm run server:prod && npm run client:prod",
    "client:prod": "ng build --prod --output-path=prod/client",
    "client:dev": "ng build --output-path=dev/client",
    "client:serve": "ng serve --proxy-config proxy.conf.json",
    "server:watch": "tsc -p ./server && tsc -w -p ./server",
    "server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dev/server/bin/www.js --watch dev/server\" ",
    "server:prod": "tsc -p ./server/tsconfig.prod.json",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test --watch=false",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "deploy": "shipit pi deploy"
  },
  "author": "Ben Steele",
  "repository": {
    "url": ""
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.2.3",
    "@angular/compiler": "2.2.3",
    "@angular/compiler-cli": "2.2.3",
    "@angular/core": "2.2.3",
    "@angular/forms": "2.2.3",
    "@angular/http": "2.2.3",
    "@angular/platform-browser": "2.2.3",
    "@angular/platform-browser-dynamic": "2.2.3",
    "@angular/router": "3.2.3",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.0",
    "@ngrx/store": "^2.2.1",
    "@ngrx/store-devtools": "^3.2.1",
    "@ngrx/store-log-monitor": "^3.0.2",
    "@types/body-parser": "0.0.32",
    "@types/bootstrap": "^3.3.32",
    "@types/compression": "0.0.33",
    "@types/cors": "0.0.33",
    "@types/event-stream": "^3.3.30",
    "@types/express": "^4.0.32",
    "@types/jasmine": "^2.2.30",
    "@types/jquery": "^2.0.34",
    "@types/jsonstream": "^0.8.28",
    "@types/jsonwebtoken": "^7.1.31",
    "@types/morgan": "^1.7.32",
    "@types/node": "^6.0.48",
